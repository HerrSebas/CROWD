<!--/events or event#index-->
<div class="container-form">

  <h1>event#index</h1>
  <p>Find mi in app/views/events/index</p>
  <ul>
    <% @events.each do |event| %>
    <li><%= link_to event.event_name, event_path(event.id) %></li>
    <li><%=event.event_date%></li>
    <li><%=event.event_location%></li>
    <% end  %>
  </ul>

</div>
